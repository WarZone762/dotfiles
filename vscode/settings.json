{
  "editor.fontFamily": "'CaskaydiaCove NF', 'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": true,
  "editor.lineNumbers": "relative",
  "editor.maxTokenizationLineLength": 60000,
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.profiles.windows": {
    "MinGW64 zsh": {
      "path": "zsh",
      "args": [
        "--login",
        "-i"
      ],
      "env": {
        "MSYSTEM": "MINGW64",
        "CHERE_INVOKING": "1",
        "MSYS2_PATH_TYPE": "inherit"
      },
      "icon": "terminal-bash"
    }
  },
  "terminal.integrated.defaultProfile.windows": "MinGW64 zsh",
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "Lua.telemetry.enable": false,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "glualint.linter": "D:\\Users\\User\\Programs\\glualint-1.20.0-Windows\\glualint.exe",
  "javascript.format.semicolons": "insert",
  "javascript.preferences.quoteStyle": "double",
  "python.analysis.typeCheckingMode": "strict",
  "typescript.surveys.enabled": false,
  "vscode-neovim.neovimExecutablePaths.linux": "/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.win32": "nvim"
}